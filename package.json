{
  "name": "dslm_vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "nodemon --exec 'vue-cli-service serve --mode development'",
    "remove": "rm -rdf ./../server/views || exit 0",
    "copy": "cp -R dist ../server/views",
    "bundle": "npm run build && npm run remove && npm run copy",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@dongido/vue-viaudio": "*",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5",
    "@fortawesome/free-solid-svg-icons": "^5",
    "@fortawesome/vue-fontawesome": "2.0.2",
    "ajv": "*",
    "ajv-error-parser": "*",
    "axios": "^0.21.1",
    "bulma": "*",
    "bulma-checkradio": "*",
    "bulma-switch": "*",
    "core-js": "^3.6.5",
    "downloadjs": "*",
    "es6-promise": "^4.2.8",
    "vue": "^2.6.11",
    "vue-cli": "^2.9.6",
    "vue-fullscreen": "2.6.1",
    "vue-json-pretty": "1.8.1",
    "vue-keypress": "*",
    "vue-router": "^3.5.2",
    "vue-toastification": "1.7.11",
    "vue2-code-editor": "*",
    "yaml": "*"
  },
  "devDependencies": {
    "@vue/cli": "~4.5.13",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "sass": "*",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "~2.0.12",
    "sass-loader": "10.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
